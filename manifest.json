{
  "name": "FireStore Selector",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": { "48": "icon48.png",
            "128": "icon128.png" },
  "description": "Use selection by key in FireStore.",
  "browser_action" : {
    "default_icon": "icon19.png"
  },
  "permissions": [
    "http://*/*", "https://*/*"
  ],
  "content_scripts": [
      {
          "matches": [
            "https://console.firebase.google.com/*"
          ],
          "js": ["jquery-3.5.1.min.js", "myscript.js"]
      }
  ]
}
